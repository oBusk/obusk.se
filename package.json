{
    "name": "@obusk/obusk.se",
    "private": true,
    "version": "0.0.1",
    "description": "üôã‚Äç‚ôÇüîß My simple website",
    "keywords": [
        "personal",
        "vercel"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/oBusk/obusk.se.git"
    },
    "author": "Oscar Busk <oscar.busk@gmail.com>",
    "license": "ISC",
    "engines": {
        "node": "24.x"
    },
    "devEngines": {
        "runtime": {
            "name": "node",
            "version": "^24.11.1",
            "onFail": "download"
        }
    },
    "packageManager": "pnpm@10.24.0",
    "lint-staged": {
        "**/*.{ts,tsx,js,jsx}": [
            "eslint --fix"
        ],
        "**/*.{md,yml,yaml,json}": [
            "prettier  --ignore-path .prettierignore --write"
        ]
    },
    "simple-git-hooks": {
        "pre-commit": "pnpm run lint-staged"
    },
    "scripts": {
        "dev": "next",
        "build": "next build",
        "start": "next start",
        "type-check": "tsc",
        "lint": "next lint --ignore-path .gitignore",
        "postlint": "pnpm run prettier",
        "lint-fix": "next lint --ignore-path .gitignore --fix",
        "prettier": "prettier **/*.{md,yml,yaml,json} --ignore-path .gitignore  --ignore-path .prettierignore --check",
        "postlint-fix": "pnpm run prettier-fix",
        "prettier-fix": "prettier **/*.{md,yml,yaml,json} --ignore-path .gitignore  --ignore-path .prettierignore --write",
        "lint-staged": "lint-staged"
    },
    "dependencies": {
        "next": "^14.2.35",
        "normalize.css": "^8.0.1",
        "react": "^18.3.1",
        "react-div-100vh": "^0.7.0",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/node": "^18.19.130",
        "@types/react": "^18.3.27",
        "@types/react-dom": "^18.3.7",
        "@typescript-eslint/eslint-plugin": "~5.38.1",
        "@typescript-eslint/parser": "~5.38.1",
        "eslint": "^8.57.1",
        "eslint-config-next": "^14.2.33",
        "eslint-config-prettier": "^8.10.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^4.2.5",
        "lint-staged": "^13.3.0",
        "prettier": "^2.8.8",
        "simple-git-hooks": "^2.13.1",
        "typescript": "4.8.4"
    }
}
